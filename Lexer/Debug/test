class TestClass

  @@staticField  = 99

  def initialize (x, y)
    @xField, @yField = x, y
  end

  protected
  def method1
    puts @xField
  end

  private
  def method2( param )
    puts param
  end
end

class TestChild < TestClass
  def childMethod
    puts @@staticField
    puts @xField
    return 55
  end

  def method1
    super
  end
end

object = TestChild.new(3,4)
object.childMethod
object.method1
